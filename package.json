{
  "name": "@adbl/bullet",
  "version": "0.0.29",
  "description": "Library for creating web apps with JSX and web components",
  "private": false,
  "type": "module",
  "module": "./index.js",
  "typings": "./types/index.d.ts",
  "main": "library/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node scripts/build.js"
  },
  "keywords": [
    "web-components",
    "web",
    "javascript"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./library/jsx-runtime": {
      "import": "./library/jsx-runtime.js",
      "require": "./library/jsx-runtime.js",
      "types": "./types/jsx-runtime.d.ts"
    },
    "./library/hmr": {
      "import": "./library/hmr.js",
      "require": "./library/hmr.js"
    },
    "./plugin": {
      "import": "./library/plugin.js",
      "require": "./library/plugin.js",
      "types": "./types/plugin.d.ts"
    },
    "./shim": {
      "import": "./library/shim.js",
      "require": "./library/shim.js",
      "types": "./types/shim.d.ts"
    }
  },
  "homepage": "https://github.com/adebola-io/bullet#readme",
  "bugs": {
    "url": "https://github.com/adebola-io/bullet/issues"
  },
  "author": "Adebola Akomolafe",
  "repository": {
    "directory": "github.com/adebola-io/bullet",
    "type": "git"
  },
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@adbl/cells": "latest",
    "csstype": "^3.1.3"
  }
}
